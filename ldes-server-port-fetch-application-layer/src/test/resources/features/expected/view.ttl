@prefix dcat:                <http://www.w3.org/ns/dcat#> .
@prefix ldes:                <https://w3id.org/ldes#> .
@prefix mobility-hindrances: <http://localhost:8080/mobility-hindrances/> .
@prefix prov:                <http://www.w3.org/ns/prov#> .
@prefix rdf:                 <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix terms:               <http://purl.org/dc/terms/> .
@prefix tree:                <https://w3id.org/tree#> .

<http://localhost:8080/mobility-hindrances/by-page/description>
        rdf:type            dcat:DataService ;
        dcat:endpointURL    mobility-hindrances:by-page ;
        dcat:servesDataset  <http://localhost:8080/mobility-hindrances> .

mobility-hindrances:MobilityHindranceShape
        rdf:type  "https://www.w3.org/ns/shacl#NodeShape" .

mobility-hindrances:by-page
        rdf:type       tree:Node ;
        tree:relation  [ rdf:type   tree:Relation ;
                         tree:node  <http://localhost:8080/mobility-hindrances/by-page?page=1>
                       ] .

<http://localhost:8080/mobility-hindrances>
        rdf:type            ldes:EventStream ;
        ldes:timestampPath  prov:generatedAtTime ;
        ldes:versionOfPath  terms:isVersionOf ;
        tree:view           mobility-hindrances:by-page .