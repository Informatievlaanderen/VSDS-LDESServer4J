@prefix ldes: <https://w3id.org/ldes#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix sh:   <http://www.w3.org/ns/shacl#> .
@prefix tree: <https://w3id.org/tree#> .
@prefix example: <http://example.org/> .
@prefix custom: <http://example.org/> .
@prefix server: <http://localhost:8080/> .
@prefix col: <http://localhost:8080/name1/> .
@prefix col2: <http://localhost:8080/name2/> .

server:name1 a ldes:EventStream ;
    ldes:timestampPath dcterms:created ;
    ldes:versionOfPath dcterms:isVersionOf ;
    ldes:view col:view2, col:view3 ;
    custom:memberType <https://data.vlaanderen.be/ns/mobiliteit#Mobiliteitshinder> ;
    tree:shape server:shape .


server:name2 a ldes:EventStream ;
    ldes:timestampPath dcterms:created ;
    ldes:versionOfPath dcterms:isVersionOf ;
    ldes:view col2:view1 ;
    custom:memberType <https://data.vlaanderen.be/ns/mobiliteit#Mobiliteitshinder> ;
    tree:shape server:shape .


server:shape a sh:NodeShape ;
   sh:nodeShape [
     sh:closed true ;
     sh:propertyShape []
     ] ;
   sh:deactivated true .

col2:view1 tree:viewDescription [
   example:fragmentationStrategy [
       a example:Fragmentation ;
       example:name "fragmentationStrategy" ;
       example:property "ldes:propertyPath"
   ]
] .

col:view2 tree:viewDescription [
   example:fragmentationStrategy [
       a example:Fragmentation ;
       example:name "fragmentationStrategy" ;
       example:property "ldes:propertyPath"
   ]
] .

col:view3 tree:viewDescription [
   example:fragmentationStrategy [
       a example:Fragmentation ;
       example:name "fragmentationStrategy" ;
       example:property "ldes:propertyPath"
   ]
] .
